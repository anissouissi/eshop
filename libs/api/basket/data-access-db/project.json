{
  "name": "api-basket-data-access-db",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/basket/data-access-db/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/api/basket/data-access-db",
        "main": "libs/api/basket/data-access-db/src/index.ts",
        "tsConfig": "libs/api/basket/data-access-db/tsconfig.lib.json",
        "assets": ["libs/api/basket/data-access-db/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "generate-db-types": {
      "executor": "@aso/automation:prisma-generate",
      "options": {
        "schemaPath": "libs/api/basket/data-access-db/src/lib/schema.prisma",
        "prismaClientImport": "@prisma/basket",
        "prismaGeneratedIndexFilePath": "libs/api/basket/generated-db-types/src/index.ts"
      }
    },
    "push-db": {
      "executor": "@aso/automation:prisma-push",
      "options": {
        "schemaPath": "libs/api/basket/data-access-db/src/lib/schema.prisma"
      }
    }
  },
  "tags": ["scope:api"]
}
